#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x55b1316e69f0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x55b1316e6b80 .scope module, "test_halfadder" "test_halfadder" 3 3;
 .timescale 0 0;
v0x55b1316fa770_0 .var "a", 0 0;
v0x55b1316fa830_0 .var "b", 0 0;
v0x55b1316fa900_0 .net "c", 0 0, v0x55b1316fa530_0;  1 drivers
v0x55b1316faa00_0 .net "s", 0 0, v0x55b1316fa600_0;  1 drivers
S_0x55b1316accf0 .scope module, "_halfadder" "halfadder" 3 7, 4 1 0, S_0x55b1316e6b80;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "s";
    .port_info 3 /OUTPUT 1 "c";
v0x55b1316aeba0_0 .net "a", 0 0, v0x55b1316fa770_0;  1 drivers
v0x55b1316fa470_0 .net "b", 0 0, v0x55b1316fa830_0;  1 drivers
v0x55b1316fa530_0 .var "c", 0 0;
v0x55b1316fa600_0 .var "s", 0 0;
E_0x55b1316e6000 .event edge, v0x55b1316aeba0_0, v0x55b1316fa470_0;
    .scope S_0x55b1316accf0;
T_0 ;
Ewait_0 .event/or E_0x55b1316e6000, E_0x0;
    %wait Ewait_0;
    %load/vec4 v0x55b1316aeba0_0;
    %load/vec4 v0x55b1316fa470_0;
    %xor;
    %store/vec4 v0x55b1316fa600_0, 0, 1;
    %load/vec4 v0x55b1316aeba0_0;
    %load/vec4 v0x55b1316fa470_0;
    %and;
    %store/vec4 v0x55b1316fa530_0, 0, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55b1316e6b80;
T_1 ;
    %vpi_call/w 3 15 "$display", "a b || s c" {0 0 0};
    %vpi_call/w 3 16 "$display", "----++----" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b1316fa770_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b1316fa830_0, 0, 1;
    %delay 10, 0;
    %vpi_call/w 3 18 "$display", "%d %d || %d %d", v0x55b1316fa770_0, v0x55b1316fa830_0, v0x55b1316faa00_0, v0x55b1316fa900_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b1316fa770_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55b1316fa830_0, 0, 1;
    %delay 10, 0;
    %vpi_call/w 3 19 "$display", "%d %d || %d %d", v0x55b1316fa770_0, v0x55b1316fa830_0, v0x55b1316faa00_0, v0x55b1316fa900_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55b1316fa770_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b1316fa830_0, 0, 1;
    %delay 10, 0;
    %vpi_call/w 3 20 "$display", "%d %d || %d %d", v0x55b1316fa770_0, v0x55b1316fa830_0, v0x55b1316faa00_0, v0x55b1316fa900_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55b1316fa770_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55b1316fa830_0, 0, 1;
    %delay 10, 0;
    %vpi_call/w 3 21 "$display", "%d %d || %d %d", v0x55b1316fa770_0, v0x55b1316fa830_0, v0x55b1316faa00_0, v0x55b1316fa900_0 {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "-";
    "test_halfadder.sv";
